{
  "name": "vue-tap-away",
  "version": "0.1.10",
  "description": "Reusable tap-away directive to detect taps/clicks outside an element. Works with touch devices!",
  "keywords": [
    "vue-tap-away",
    "click-outside",
    "click-away",
    "outside click"
  ],
  "browserslist": [
    "defaults",
    "Safari 11",
    "Safari 10"
  ],
  "scripts": {
    "mix": "cross-env-shell node_modules/webpack/bin/webpack.js --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "install": "cross-env NODE_ENV=production yarn run mix"
  },
  "homepage": "https://github.com/aariacarterweir/vue-tap-away",
  "bugs": "https://github.com/aariacarterweir/vue-tap-away/issues",
  "license": "MIT",
  "author": "Aaria Carter-Weir",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aariacarterweir/vue-tap-away.git"
  },
  "dependencies": {
    "@babel/parser": "7.9.6",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "event-propagation-path": "^1.0.5",
    "hammerjs": "^2.0.8",
    "laravel-mix": "^5.0.4",
    "lodash": "^4.17.15",
    "propagating-hammerjs": "^1.4.7",
    "vue-template-compiler": "^2.6.10"
  },
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
}
