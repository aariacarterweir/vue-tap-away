{
  "name": "vue-tap-away",
  "version": "0.2.0",
  "description": "Reusable tap-away directive to detect taps/clicks outside an element. Works with touch devices!",
  "keywords": [
    "vue-tap-away",
    "click-outside",
    "click-away",
    "outside click"
  ],
  "browserslist": [
    "defaults",
    "Safari 11",
    "Safari 10"
  ],
  "scripts": {
    "build": "cross-env-shell node_modules/webpack/bin/webpack.js --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prepublish": "cross-env NODE_ENV=production yarn run build"
  },
  "homepage": "https://github.com/aariacarterweir/vue-tap-away",
  "bugs": "https://github.com/aariacarterweir/vue-tap-away/issues",
  "license": "MIT",
  "author": "Aaria Carter-Weir",
  "main": "dist/main.js",
  "module": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aariacarterweir/vue-tap-away.git"
  },
  "dependencies": {},
  "peerDependencies": {
    "event-propagation-path": "^1.0.5",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "propagating-hammerjs": "^1.4.7"
  },
  "devDependencies": {
    "@babel/parser": "7.11.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "laravel-mix": "^5.0.5",
    "vue-template-compiler": "^2.6.12"
  }
}
